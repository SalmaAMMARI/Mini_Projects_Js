{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport \"core-js/modules/es.iterator.reduce.js\";\nimport \"core-js/modules/es.set.difference.v2.js\";\nimport \"core-js/modules/es.set.intersection.v2.js\";\nimport \"core-js/modules/es.set.is-disjoint-from.v2.js\";\nimport \"core-js/modules/es.set.is-subset-of.v2.js\";\nimport \"core-js/modules/es.set.is-superset-of.v2.js\";\nimport \"core-js/modules/es.set.symmetric-difference.v2.js\";\nimport \"core-js/modules/es.set.union.v2.js\";\nexport default {\n  name: 'App',\n  data() {\n    return {\n      searchQuery: '',\n      filterByCategory: 'all',\n      // 'all' ou une cat√©gorie sp√©cifique\n      filterByAvailability: 'all',\n      // 'all', 'available', 'unavailable'\n      sortOrder: 'asc',\n      // 'asc' pour croissant, 'desc' pour d√©croissant\n      panier: [],\n      // Tableau pour stocker les produits ajout√©s au panier\n      produits: [{\n        \"id\": 1,\n        \"nom\": \"Batterie 12V\",\n        \"prix\": 120,\n        \"categorie\": \"√âlectricit√©\",\n        \"Image\": \"image_batterie_12v.jpg\",\n        \"Disponible\": true\n      }, {\n        \"id\": 2,\n        \"nom\": \"Filtre √† huile\",\n        \"prix\": 15,\n        \"categorie\": \"Filtration\",\n        \"Image\": \"image_filtre_huile.jpg\",\n        \"Disponible\": true\n      }, {\n        \"id\": 3,\n        \"nom\": \"Bougies d'allumage (x4)\",\n        \"prix\": 35,\n        \"categorie\": \"Moteur\",\n        \"Image\": \"image_bougies_allumage.jpg\",\n        \"Disponible\": false\n      }, {\n        \"id\": 4,\n        \"nom\": \"Disques de frein (x2)\",\n        \"prix\": 80,\n        \"categorie\": \"Freinage\",\n        \"Image\": \"image_disques_frein.jpg\",\n        \"Disponible\": true\n      }, {\n        \"id\": 5,\n        \"nom\": \"Courroie de distribution\",\n        \"prix\": 90,\n        \"categorie\": \"Moteur\",\n        \"Image\": \"image_courroie_distribution.jpg\",\n        \"Disponible\": false\n      }, {\n        \"id\": 6,\n        \"nom\": \"Pompe √† eau\",\n        \"prix\": 70,\n        \"categorie\": \"Refroidissement\",\n        \"Image\": \"image_pompe_eau.jpg\",\n        \"Disponible\": true\n      }, {\n        \"id\": 7,\n        \"nom\": \"Amortisseurs arri√®re (x2)\",\n        \"prix\": 150,\n        \"categorie\": \"Suspension\",\n        \"Image\": \"image_amortisseurs_arriere.jpg\",\n        \"Disponible\": true\n      }, {\n        \"id\": 8,\n        \"nom\": \"Filtre √† air\",\n        \"prix\": 20,\n        \"categorie\": \"Filtration\",\n        \"Image\": \"image_filtre_air.jpg\",\n        \"Disponible\": true\n      }, {\n        \"id\": 9,\n        \"nom\": \"Capteur ABS\",\n        \"prix\": 50,\n        \"categorie\": \"S√©curit√©\",\n        \"Image\": \"image_capteur_abs.jpg\",\n        \"Disponible\": false\n      }, {\n        \"id\": 10,\n        \"nom\": \"Radiateur de refroidissement\",\n        \"prix\": 130,\n        \"categorie\": \"Refroidissement\",\n        \"Image\": \"image_radiateur_refroidissement.jpg\",\n        \"Disponible\": true\n      }, {\n        \"id\": 11,\n        \"nom\": \"Alternateur\",\n        \"prix\": 200,\n        \"categorie\": \"√âlectricit√©\",\n        \"Image\": \"image_alternateur.jpg\",\n        \"Disponible\": true\n      }, {\n        \"id\": 12,\n        \"nom\": \"D√©marreur\",\n        \"prix\": 180,\n        \"categorie\": \"√âlectricit√©\",\n        \"Image\": \"image_demarreur.jpg\",\n        \"Disponible\": false\n      }, {\n        \"id\": 13,\n        \"nom\": \"Kit d‚Äôembrayage\",\n        \"prix\": 220,\n        \"categorie\": \"Transmission\",\n        \"Image\": \"image_kit_embrayage.jpg\",\n        \"Disponible\": true\n      }, {\n        \"id\": 14,\n        \"nom\": \"Injecteur de carburant\",\n        \"prix\": 110,\n        \"categorie\": \"Moteur\",\n        \"Image\": \"image_injecteur_carburant.jpg\",\n        \"Disponible\": true\n      }, {\n        \"id\": 15,\n        \"nom\": \"Pompe √† carburant\",\n        \"prix\": 90,\n        \"categorie\": \"Carburant\",\n        \"Image\": \"image_pompe_carburant.jpg\",\n        \"Disponible\": true\n      }, {\n        \"id\": 16,\n        \"nom\": \"Capteur de pression des pneus (TPMS)\",\n        \"prix\": 45,\n        \"categorie\": \"S√©curit√©\",\n        \"Image\": \"image_capteur_tpms.jpg\",\n        \"Disponible\": true\n      }, {\n        \"id\": 17,\n        \"nom\": \"R√©troviseur ext√©rieur\",\n        \"prix\": 60,\n        \"categorie\": \"Carrosserie\",\n        \"Image\": \"image_retroviseur_exterieur.jpg\",\n        \"Disponible\": false\n      }, {\n        \"id\": 18,\n        \"nom\": \"√âchappement complet\",\n        \"prix\": 250,\n        \"categorie\": \"√âchappement\",\n        \"Image\": \"image_echappement_complet.jpg\",\n        \"Disponible\": true\n      }, {\n        \"id\": 19,\n        \"nom\": \"Vanne EGR\",\n        \"prix\": 140,\n        \"categorie\": \"Moteur\",\n        \"Image\": \"image_vanne_egr.jpg\",\n        \"Disponible\": true\n      }, {\n        \"id\": 20,\n        \"nom\": \"Turbo\",\n        \"prix\": 400,\n        \"categorie\": \"Moteur\",\n        \"Image\": \"image_turbo.jpg\",\n        \"Disponible\": true\n      }, {\n        \"id\": 21,\n        \"nom\": \"Joint de culasse\",\n        \"prix\": 75,\n        \"categorie\": \"Moteur\",\n        \"Image\": \"image_joint_culasse.jpg\",\n        \"Disponible\": false\n      }, {\n        \"id\": 22,\n        \"nom\": \"Bo√Ætier de direction\",\n        \"prix\": 300,\n        \"categorie\": \"Direction\",\n        \"Image\": \"image_bo√Ætier_direction.jpg\",\n        \"Disponible\": true\n      }, {\n        \"id\": 23,\n        \"nom\": \"Silent bloc de suspension\",\n        \"prix\": 35,\n        \"categorie\": \"Suspension\",\n        \"Image\": \"image_silent_bloc_suspension.jpg\",\n        \"Disponible\": true\n      }, {\n        \"id\": 24,\n        \"nom\": \"injecteur carburant\",\n        \"prix\": 160,\n        \"categorie\": \"Transmission\",\n        \"Image\": \"image_injecteur_carburant.jpg\",\n        \"Disponible\": true\n      }, {\n        \"id\": 25,\n        \"nom\": \"Sileimage cardan transmission\",\n        \"prix\": 50,\n        \"categorie\": \"Moteur\",\n        \"Image\": \"image_sileimage_cardan_transmission.jpg\",\n        \"Disponible\": true\n      }]\n    };\n  },\n  computed: {\n    // Liste des cat√©gories uniques\n    categories() {\n      return [...new Set(this.produits.map(produit => produit.categorie))];\n    },\n    // Produits filtr√©s et tri√©s\n    filteredProduits() {\n      let produits = this.produits;\n\n      // Filtrage par recherche\n      if (this.searchQuery) {\n        produits = produits.filter(produit => produit.nom.toLowerCase().includes(this.searchQuery.toLowerCase()));\n      }\n\n      // Filtrage par cat√©gorie\n      if (this.filterByCategory !== 'all') {\n        produits = produits.filter(produit => produit.categorie === this.filterByCategory);\n      }\n\n      // Filtrage par disponibilit√©\n      if (this.filterByAvailability === 'available') {\n        produits = produits.filter(produit => produit.Disponible);\n      } else if (this.filterByAvailability === 'unavailable') {\n        produits = produits.filter(produit => !produit.Disponible);\n      }\n\n      // Tri par prix\n      if (this.sortOrder === 'asc') {\n        produits.sort((a, b) => a.prix - b.prix); // Prix croissant\n      } else if (this.sortOrder === 'desc') {\n        produits.sort((a, b) => b.prix - a.prix); // Prix d√©croissant\n      }\n      return produits;\n    },\n    // Calcul du prix total du panier\n    prixTotal() {\n      return this.panier.reduce((total, item) => total + item.produit.prix * item.quantite, 0);\n    }\n  },\n  methods: {\n    // Ajouter un produit au panier\n    ajouterAuPanier(produit) {\n      const index = this.panier.findIndex(item => item.produit.id === produit.id);\n      if (index !== -1) {\n        // Si le produit est d√©j√† dans le panier, augmenter la quantit√©\n        this.panier[index].quantite++;\n      } else {\n        // Sinon, ajouter le produit au panier avec une quantit√© de 1\n        this.panier.push({\n          produit,\n          quantite: 1\n        });\n      }\n    },\n    // Supprimer un produit du panier ou d√©cr√©menter la quantit√©\n    supprimerDuPanier(index) {\n      if (this.panier[index].quantite > 1) {\n        // Si la quantit√© est sup√©rieure √† 1, d√©cr√©menter la quantit√©\n        this.panier[index].quantite--;\n      } else {\n        // Si la quantit√© est √©gale √† 1, supprimer le produit du panier\n        this.panier.splice(index, 1);\n      }\n    }\n  }\n};","map":{"version":3,"names":["name","data","searchQuery","filterByCategory","filterByAvailability","sortOrder","panier","produits","computed","categories","Set","map","produit","categorie","filteredProduits","filter","nom","toLowerCase","includes","Disponible","sort","a","b","prix","prixTotal","reduce","total","item","quantite","methods","ajouterAuPanier","index","findIndex","id","push","supprimerDuPanier","splice"],"sources":["/workspaces/Mini_Projects_Js/src/components/HelloWorld.vue"],"sourcesContent":["<template>\n  <div>\n    <!-- En-t√™te -->\n    <header>\n      <div class=\"logo-container\">\n        <img src=\"logo.jpg\" alt=\"LOGO\" />\n        <h1>Les Bonnes Pi√®ces</h1>\n      </div>\n    </header>\n\n    <!-- Section principale -->\n    <main>\n      <!-- Menu de recherche (Filtre) -->\n      <aside class=\"filtres\">\n        <h3>Filtres</h3>\n        <div class=\"input-container\">\n          <input \n            type=\"text\" \n            v-model=\"searchQuery\" \n            placeholder=\"üîç Recherchez un produit...\" \n          />\n        </div>\n\n        <!-- Filtrage par cat√©gorie -->\n        <div class=\"filter-section\">\n          <h4>Filtrer par cat√©gorie</h4>\n          <select v-model=\"filterByCategory\">\n            <option value=\"all\">Toutes cat√©gories</option>\n            <option v-for=\"categorie in categories\" :key=\"categorie\" :value=\"categorie\">\n              {{ categorie }}\n            </option>\n          </select>\n        </div>\n\n        <!-- Filtrage par disponibilit√© -->\n        <div class=\"filter-section\">\n          <h4>Filtrer par disponibilit√©</h4>\n          <select v-model=\"filterByAvailability\">\n            <option value=\"all\">Tous</option>\n            <option value=\"available\">Disponibles</option>\n            <option value=\"unavailable\">Indisponibles</option>\n          </select>\n        </div>\n\n        <!-- Tri par prix -->\n        <div class=\"filter-section\">\n          <h4>Trier par prix</h4>\n          <select v-model=\"sortOrder\">\n            <option value=\"asc\">Prix croissant</option>\n            <option value=\"desc\">Prix d√©croissant</option>\n          </select>\n        </div>\n      </aside>\n\n      <!-- Fiches produits -->\n      <section class=\"fiches\">\n        <div v-for=\"produit in filteredProduits\" :key=\"produit.id\" class=\"produit\">\n          <img :src=\"produit.Image\" :alt=\"produit.nom\" />\n          <h3>{{ produit.nom }}</h3>\n          <p class=\"prix\">Prix : {{ produit.prix }} Dhs</p>\n          <p v-if=\"produit.Disponible\" class=\"disponible\">‚úÖ Disponible</p>\n          <p v-else class=\"indisponible\">‚ùå Indisponible</p>\n          <button @click=\"ajouterAuPanier(produit)\" class=\"but\">Ajouter au panier</button>\n        </div>\n      </section>\n\n      <!-- Panier -->\n      <section class=\"panier\">\n        <h3>Panier</h3>\n        <div v-if=\"panier.length === 0\">\n          <p>Votre panier est vide.</p>\n        </div>\n        <div v-else>\n          <div v-for=\"(item, index) in panier\" :key=\"item.produit.id\" class=\"panier-item\">\n            <h4>{{ item.produit.nom }}</h4>\n            <p>Prix unitaire : {{ item.produit.prix }} Dhs</p>\n            <p>Quantit√© : {{ item.quantite }}</p>\n            <p>Total : {{ item.produit.prix * item.quantite }} Dhs</p>\n            <button @click=\"supprimerDuPanier(index)\">Supprimer</button>\n          </div>\n          <p class=\"prix-total\">Prix total : {{ prixTotal }} Dhs</p>\n        </div>\n      </section>\n    </main>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'App',\n  data() {\n    return {\n      searchQuery: '',\n      filterByCategory: 'all', // 'all' ou une cat√©gorie sp√©cifique\n      filterByAvailability: 'all', // 'all', 'available', 'unavailable'\n      sortOrder: 'asc', // 'asc' pour croissant, 'desc' pour d√©croissant\n      panier: [], // Tableau pour stocker les produits ajout√©s au panier\n      produits: [\n        {\n          \"id\": 1,\n          \"nom\": \"Batterie 12V\",\n          \"prix\": 120,\n          \"categorie\": \"√âlectricit√©\",\n          \"Image\": \"image_batterie_12v.jpg\",\n          \"Disponible\": true\n        },\n        {\n          \"id\": 2,\n          \"nom\": \"Filtre √† huile\",\n          \"prix\": 15,\n          \"categorie\": \"Filtration\",\n          \"Image\": \"image_filtre_huile.jpg\",\n          \"Disponible\": true\n        },\n        {\n          \"id\": 3,\n          \"nom\": \"Bougies d'allumage (x4)\",\n          \"prix\": 35,\n          \"categorie\": \"Moteur\",\n          \"Image\": \"image_bougies_allumage.jpg\",\n          \"Disponible\": false\n        },\n        {\n          \"id\": 4,\n          \"nom\": \"Disques de frein (x2)\",\n          \"prix\": 80,\n          \"categorie\": \"Freinage\",\n          \"Image\": \"image_disques_frein.jpg\",\n          \"Disponible\": true\n        },\n        {\n          \"id\": 5,\n          \"nom\": \"Courroie de distribution\",\n          \"prix\": 90,\n          \"categorie\": \"Moteur\",\n          \"Image\": \"image_courroie_distribution.jpg\",\n          \"Disponible\": false\n        },\n        {\n          \"id\": 6,\n          \"nom\": \"Pompe √† eau\",\n          \"prix\": 70,\n          \"categorie\": \"Refroidissement\",\n          \"Image\": \"image_pompe_eau.jpg\",\n          \"Disponible\": true\n        },\n        {\n          \"id\": 7,\n          \"nom\": \"Amortisseurs arri√®re (x2)\",\n          \"prix\": 150,\n          \"categorie\": \"Suspension\",\n          \"Image\": \"image_amortisseurs_arriere.jpg\",\n          \"Disponible\": true\n        },\n        {\n          \"id\": 8,\n          \"nom\": \"Filtre √† air\",\n          \"prix\": 20,\n          \"categorie\": \"Filtration\",\n          \"Image\": \"image_filtre_air.jpg\",\n          \"Disponible\": true\n        },\n        {\n          \"id\": 9,\n          \"nom\": \"Capteur ABS\",\n          \"prix\": 50,\n          \"categorie\": \"S√©curit√©\",\n          \"Image\": \"image_capteur_abs.jpg\",\n          \"Disponible\": false\n        },\n        {\n          \"id\": 10,\n          \"nom\": \"Radiateur de refroidissement\",\n          \"prix\": 130,\n          \"categorie\": \"Refroidissement\",\n          \"Image\": \"image_radiateur_refroidissement.jpg\",\n          \"Disponible\": true\n        },\n        {\n          \"id\": 11,\n          \"nom\": \"Alternateur\",\n          \"prix\": 200,\n          \"categorie\": \"√âlectricit√©\",\n          \"Image\": \"image_alternateur.jpg\",\n          \"Disponible\": true\n        },\n        {\n          \"id\": 12,\n          \"nom\": \"D√©marreur\",\n          \"prix\": 180,\n          \"categorie\": \"√âlectricit√©\",\n          \"Image\": \"image_demarreur.jpg\",\n          \"Disponible\": false\n        },\n        {\n          \"id\": 13,\n          \"nom\": \"Kit d‚Äôembrayage\",\n          \"prix\": 220,\n          \"categorie\": \"Transmission\",\n          \"Image\": \"image_kit_embrayage.jpg\",\n          \"Disponible\": true\n        },\n        {\n          \"id\": 14,\n          \"nom\": \"Injecteur de carburant\",\n          \"prix\": 110,\n          \"categorie\": \"Moteur\",\n          \"Image\": \"image_injecteur_carburant.jpg\",\n          \"Disponible\": true\n        },\n        {\n          \"id\": 15,\n          \"nom\": \"Pompe √† carburant\",\n          \"prix\": 90,\n          \"categorie\": \"Carburant\",\n          \"Image\": \"image_pompe_carburant.jpg\",\n          \"Disponible\": true\n        },\n        {\n          \"id\": 16,\n          \"nom\": \"Capteur de pression des pneus (TPMS)\",\n          \"prix\": 45,\n          \"categorie\": \"S√©curit√©\",\n          \"Image\": \"image_capteur_tpms.jpg\",\n          \"Disponible\": true\n        },\n        {\n          \"id\": 17,\n          \"nom\": \"R√©troviseur ext√©rieur\",\n          \"prix\": 60,\n          \"categorie\": \"Carrosserie\",\n          \"Image\": \"image_retroviseur_exterieur.jpg\",\n          \"Disponible\": false\n        },\n        {\n          \"id\": 18,\n          \"nom\": \"√âchappement complet\",\n          \"prix\": 250,\n          \"categorie\": \"√âchappement\",\n          \"Image\": \"image_echappement_complet.jpg\",\n          \"Disponible\": true\n        },\n        {\n          \"id\": 19,\n          \"nom\": \"Vanne EGR\",\n          \"prix\": 140,\n          \"categorie\": \"Moteur\",\n          \"Image\": \"image_vanne_egr.jpg\",\n          \"Disponible\": true\n        },\n        {\n          \"id\": 20,\n          \"nom\": \"Turbo\",\n          \"prix\": 400,\n          \"categorie\": \"Moteur\",\n          \"Image\": \"image_turbo.jpg\",\n          \"Disponible\": true\n        },\n        {\n          \"id\": 21,\n          \"nom\": \"Joint de culasse\",\n          \"prix\": 75,\n          \"categorie\": \"Moteur\",\n          \"Image\": \"image_joint_culasse.jpg\",\n          \"Disponible\": false\n        },\n        {\n          \"id\": 22,\n          \"nom\": \"Bo√Ætier de direction\",\n          \"prix\": 300,\n          \"categorie\": \"Direction\",\n          \"Image\": \"image_bo√Ætier_direction.jpg\",\n          \"Disponible\": true\n        },\n        {\n          \"id\": 23,\n          \"nom\": \"Silent bloc de suspension\",\n          \"prix\": 35,\n          \"categorie\": \"Suspension\",\n          \"Image\": \"image_silent_bloc_suspension.jpg\",\n          \"Disponible\": true\n        },\n        {\n          \"id\": 24,\n          \"nom\": \"injecteur carburant\",\n          \"prix\": 160,\n          \"categorie\": \"Transmission\",\n          \"Image\": \"image_injecteur_carburant.jpg\",\n          \"Disponible\": true\n        },\n        {\n          \"id\": 25,\n          \"nom\": \"Sileimage cardan transmission\",\n          \"prix\": 50,\n          \"categorie\": \"Moteur\",\n          \"Image\": \"image_sileimage_cardan_transmission.jpg\",\n          \"Disponible\": true\n        }\n      ]\n    };\n  },\n  computed: {\n    // Liste des cat√©gories uniques\n    categories() {\n      return [...new Set(this.produits.map(produit => produit.categorie))];\n    },\n    // Produits filtr√©s et tri√©s\n    filteredProduits() {\n      let produits = this.produits;\n\n      // Filtrage par recherche\n      if (this.searchQuery) {\n        produits = produits.filter(produit =>\n          produit.nom.toLowerCase().includes(this.searchQuery.toLowerCase())\n        );\n      }\n\n      // Filtrage par cat√©gorie\n      if (this.filterByCategory !== 'all') {\n        produits = produits.filter(produit =>\n          produit.categorie === this.filterByCategory\n        );\n      }\n\n      // Filtrage par disponibilit√©\n      if (this.filterByAvailability === 'available') {\n        produits = produits.filter(produit => produit.Disponible);\n      } else if (this.filterByAvailability === 'unavailable') {\n        produits = produits.filter(produit => !produit.Disponible);\n      }\n\n      // Tri par prix\n      if (this.sortOrder === 'asc') {\n        produits.sort((a, b) => a.prix - b.prix); // Prix croissant\n      } else if (this.sortOrder === 'desc') {\n        produits.sort((a, b) => b.prix - a.prix); // Prix d√©croissant\n      }\n\n      return produits;\n    },\n    // Calcul du prix total du panier\n    prixTotal() {\n      return this.panier.reduce((total, item) => total + item.produit.prix * item.quantite, 0);\n    }\n  },\n  methods: {\n    // Ajouter un produit au panier\n    ajouterAuPanier(produit) {\n      const index = this.panier.findIndex(item => item.produit.id === produit.id);\n      if (index !== -1) {\n        // Si le produit est d√©j√† dans le panier, augmenter la quantit√©\n        this.panier[index].quantite++;\n      } else {\n        // Sinon, ajouter le produit au panier avec une quantit√© de 1\n        this.panier.push({ produit, quantite: 1 });\n      }\n    },\n    // Supprimer un produit du panier ou d√©cr√©menter la quantit√©\n    supprimerDuPanier(index) {\n      if (this.panier[index].quantite > 1) {\n        // Si la quantit√© est sup√©rieure √† 1, d√©cr√©menter la quantit√©\n        this.panier[index].quantite--;\n      } else {\n        // Si la quantit√© est √©gale √† 1, supprimer le produit du panier\n        this.panier.splice(index, 1);\n      }\n    }\n  }\n};\n</script>\n\n<style>\n/* üåü Style global */\nbody {\n  max-width: 1200px;\n  margin: auto;\n  padding: 16px;\n  font-family: 'Poppins', sans-serif;\n  background-color: #f8f9fa;\n}\n\n/* üåü En-t√™te */\nheader {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  background-color: #6C63FF;\n  color: white;\n  padding: 20px;\n  border-radius: 10px;\n  box-shadow: 0px 4px 8px rgba(0, 0, 0, 0.1);\n}\n\nheader .logo-container {\n  display: flex;\n  align-items: center;\n}\n\nheader img {\n  height: 50px;\n  margin-right: 12px;\n}\n\nheader h1 {\n  font-size: 28px;\n  font-weight: 600;\n  text-transf