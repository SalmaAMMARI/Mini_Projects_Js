ck(true), _createElementBlock(_Fragment, null, _renderList($data.panier, (item, index) => {\n    return _openBlock(), _createElementBlock(\"div\", {\n      key: item.produit.id,\n      class: \"panier-item\"\n    }, [_createElementVNode(\"h4\", null, _toDisplayString(item.produit.nom), 1 /* TEXT */), _createElementVNode(\"p\", null, \"Prix unitaire : \" + _toDisplayString(item.produit.prix) + \" Dhs\", 1 /* TEXT */), _createElementVNode(\"p\", null, \"Quantit√© : \" + _toDisplayString(item.quantite), 1 /* TEXT */), _createElementVNode(\"p\", null, \"Total : \" + _toDisplayString(item.produit.prix * item.quantite) + \" Dhs\", 1 /* TEXT */), _createElementVNode(\"button\", {\n      onClick: $event => $options.supprimerDuPanier(index)\n    }, \"Supprimer\", 8 /* PROPS */, _hoisted_16)]);\n  }), 128 /* KEYED_FRAGMENT */)), _createElementVNode(\"p\", _hoisted_17, \"Prix total : \" + _toDisplayString($options.prixTotal) + \" Dhs\", 1 /* TEXT */)]))])])]);\n}","map":{"version":3,"names":["class","key","_createElementBlock","_createCommentVNode","_createElementVNode","src","alt","_hoisted_1","_hoisted_2","type","_cache","$event","$data","searchQuery","placeholder","_hoisted_3","filterByCategory","value","_Fragment","_renderList","$options","categories","categorie","_hoisted_4","_hoisted_5","filterByAvailability","_hoisted_6","sortOrder","_hoisted_7","filteredProduits","produit","id","Image","nom","_hoisted_8","_toDisplayString","_hoisted_9","prix","Disponible","_hoisted_10","_hoisted_11","onClick","ajouterAuPanier","_hoisted_12","_hoisted_13","panier","length","_hoisted_14","_hoisted_15","item","index","quantite","supprimerDuPanier","_hoisted_16","_hoisted_17","prixTotal"],"sources":["/workspaces/Mini_Projects_Js/TP4_Views_CLI/src/components/HelloWorld.vue"],"sourcesContent":["<template>\n  <div>\n    <!-- En-t√™te -->\n    <header>\n      <div class=\"logo-container\">\n        <img src=\"logo.jpg\" alt=\"LOGO\" />\n        <h1>Les Bonnes Pi√®ces</h1>\n      </div>\n    </header>\n\n    <!-- Section principale -->\n    <main>\n      <!-- Menu de recherche (Filtre) -->\n      <aside class=\"filtres\">\n        <h3>Filtres</h3>\n        <div class=\"input-container\">\n          <input \n            type=\"text\" \n            v-model=\"searchQuery\" \n            placeholder=\"üîç Recherchez un produit...\" \n          />\n        </div>\n\n        <!-- Filtrage par cat√©gorie -->\n        <div class=\"filter-section\">\n          <h4>Filtrer par cat√©gorie</h4>\n          <select v-model=\"filterByCategory\">\n            <option value=\"all\">Toutes cat√©gories</option>\n            <option v-for=\"categorie in categories\" :key=\"categorie\" :value=\"categorie\">\n              {{ categorie }}\n            </option>\n          </select>\n        </div>\n\n        <!-- Filtrage par disponibilit√© -->\n        <div class=\"filter-section\">\n          <h4>Filtrer par disponibilit√©</h4>\n          <select v-model=\"filterByAvailability\">\n            <option value=\"all\">Tous</option>\n            <option value=\"available\">Disponibles</option>\n            <option value=\"unavailable\">Indisponibles</option>\n          </select>\n        </div>\n\n        <!-- Tri par prix -->\n        <div class=\"filter-section\">\n          <h4>Trier par prix</h4>\n          <select v-model=\"sortOrder\">\n            <option value=\"asc\">Prix croissant</option>\n            <option value=\"desc\">Prix d√©croissant</option>\n          </select>\n        </div>\n      </aside>\n\n      <!-- Fiches produits -->\n      <section class=\"fiches\">\n        <div v-for=\"produit in filteredProduits\" :key=\"produit.id\" class=\"produit\">\n          <img :src=\"produit.Image\" :alt=\"produit.nom\" />\n          <h3>{{ produit.nom }}</h3>\n          <p class=\"prix\">Prix : {{ produit.prix }} Dhs</p>\n          <p v-if=\"produit.Disponible\" class=\"disponible\">‚úÖ Disponible</p>\n          <p v-else class=\"indisponible\">‚ùå Indisponible</p>\n          <button @click=\"ajouterAuPanier(produit)\" class=\"but\">Ajouter au panier</button>\n        </div>\n      </section>\n\n      <!-- Panier -->\n      <section class=\"panier\">\n        <h3>Panier</h3>\n        <div v-if=\"panier.length === 0\">\n          <p>Votre panier est vide.</p>\n        </div>\n        <div v-else>\n          <div v-for=\"(item, index) in panier\" :key=\"item.produit.id\" class=\"panier-item\">\n            <h4>{{ item.produit.nom }}</h4>\n            <p>Prix unitaire : {{ item.produit.prix }} Dhs</p>\n            <p>Quantit√© : {{ item.quantite }}</p>\n            <p>Total : {{ item.produit.prix * item.quantite }} Dhs</p>\n            <button @click=\"supprimerDuPanier(index)\">Supprimer</button>\n          </div>\n          <p class=\"prix-total\">Prix total : {{ prixTotal }} Dhs</p>\n        </div>\n      </section>\n    </main>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'App',\n  data() {\n    return {\n      searchQuery: '',\n      filterByCategory: 'all', // 'all' ou une cat√©gorie sp√©cifique\n      filterByAvailability: 'all', // 'all', 'available', 'unavailable'\n      sortOrder: 'asc', // 'asc' pour croissant, 'desc' pour d√©croissant\n      panier: [], // Tableau pour stocker les produits ajout√©s au panier\n      produits: [\n        {\n          \"id\": 1,\n          \"nom\": \"Batterie 12V\",\n          \"prix\": 120,\n          \"categorie\": \"√âlectricit√©\",\n          \"Image\": \"image_batterie_12v.jpg\",\n          \"Disponible\": true\n        },\n        {\n          \"id\": 2,\n          \"nom\": \"Filtre √† huile\",\n          \"prix\": 15,\n          \"categorie\": \"Filtration\",\n          \"Image\": \"image_filtre_huile.jpg\",\n          \"Disponible\": true\n        },\n        {\n          \"id\": 3,\n          \"nom\": \"Bougies d'allumage (x4)\",\n          \"prix\": 35,\n          \"categorie\": \"Moteur\",\n          \"Image\": \"image_bougies_allumage.jpg\",\n          \"Disponible\": false\n        },\n        {\n          \"id\": 4,\n          \"nom\": \"Disques de frein (x2)\",\n          \"prix\": 80,\n          \"categorie\": \"Freinage\",\n          \"Image\": \"image_disques_frein.jpg\",\n          \"Disponible\": true\n        },\n        {\n          \"id\": 5,\n          \"nom\": \"Courroie de distribution\",\n          \"prix\": 90,\n          \"categorie\": \"Moteur\",\n          \"Image\": \"image_courroie_distribution.jpg\",\n          \"Disponible\": false\n        },\n        {\n          \"id\": 6,\n          \"nom\": \"Pompe √† eau\",\n          \"prix\": 70,\n          \"categorie\": \"Refroidissement\",\n          \"Image\": \"image_pompe_eau.jpg\",\n          \"Disponible\": true\n        },\n        {\n          \"id\": 7,\n          \"nom\": \"Amortisseurs arri√®re (x2)\",\n          \"prix\": 150,\n          \"categorie\": \"Suspension\",\n          \"Image\": \"image_amortisseurs_arriere.jpg\",\n          \"Disponible\": true\n        },\n        {\n          \"id\": 8,\n          \"nom\": \"Filtre √† air\",\n          \"prix\": 20,\n          \"categorie\": \"Filtration\",\n          \"Image\": \"image_filtre_air.jpg\",\n          \"Disponible\": true\n        },\n        {\n          \"id\": 9,\n          \"nom\": \"Capteur ABS\",\n          \"prix\": 50,\n          \"categorie\": \"S√©curit√©\",\n          \"Image\": \"image_capteur_abs.jpg\",\n          \"Disponible\": false\n        },\n        {\n          \"id\": 10,\n          \"nom\": \"Radiateur de refroidissement\",\n          \"prix\": 130,\n          \"categorie\": \"Refroidissement\",\n          \"Image\": \"image_radiateur_refroidissement.jpg\",\n          \"Disponible\": true\n        },\n        {\n          \"id\": 11,\n          \"nom\": \"Alternateur\",\n          \"prix\": 200,\n          \"categorie\": \"√âlectricit√©\",\n          \"Image\": \"image_alternateur.jpg\",\n          \"Disponible\": true\n        },\n        {\n          \"id\": 12,\n          \"nom\": \"D√©marreur\",\n          \"prix\": 180,\n          \"categorie\": \"√âlectricit√©\",\n          \"Image\": \"image_demarreur.jpg\",\n          \"Disponible\": false\n        },\n        {\n          \"id\": 13,\n          \"nom\": \"Kit d‚Äôembrayage\",\n          \"prix\": 220,\n          \"categorie\": \"Transmission\",\n          \"Image\": \"image_kit_embrayage.jpg\",\n          \"Disponible\": true\n        },\n        {\n          \"id\": 14,\n          \"nom\": \"Injecteur de carburant\",\n          \"prix\": 110,\n          \"categorie\": \"Moteur\",\n          \"Image\": \"image_injecteur_carburant.jpg\",\n          \"Disponible\": true\n        },\n        {\n          \"id\": 15,\n          \"nom\": \"Pompe √† carburant\",\n          \"prix\": 90,\n          \"categorie\": \"Carburant\",\n          \"Image\": \"image_pompe_carburant.jpg\",\n          \"Disponible\": true\n        },\n        {\n          \"id\": 16,\n          \"nom\": \"Capteur de pression des pneus (TPMS)\",\n          \"prix\": 45,\n          \"categorie\": \"S√©curit√©\",\n          \"Image\": \"image_capteur_tpms.jpg\",\n          \"Disponible\": true\n        },\n        {\n          \"id\": 17,\n          \"nom\": \"R√©troviseur ext√©rieur\",\n          \"prix\": 60,\n          \"categorie\": \"Carrosserie\",\n          \"Image\": \"image_retroviseur_exterieur.jpg\",\n          \"Disponible\": false\n        },\n        {\n          \"id\": 18,\n          \"nom\": \"√âchappement complet\",\n          \"prix\": 250,\n          \"categorie\": \"√âchappement\",\n          \"Image\": \"image_echappement_complet.jpg\",\n          \"Disponible\": true\n        },\n        {\n          \"id\": 19,\n          \"nom\": \"Vanne EGR\",\n          \"prix\": 140,\n          \"categorie\": \"Moteur\",\n          \"Image\": \"image_vanne_egr.jpg\",\n          \"Disponible\": true\n        },\n        {\n          \"id\": 20,\n          \"nom\": \"Turbo\",\n          \"prix\": 400,\n          \"categorie\": \"Moteur\",\n          \"Image\": \"image_turbo.jpg\",\n          \"Disponible\": true\n        },\n        {\n          \"id\": 21,\n          \"nom\": \"Joint de culasse\",\n          \"prix\": 75,\n          \"categorie\": \"Moteur\",\n          \"Image\": \"image_joint_culasse.jpg\",\n          \"Disponible\": false\n        },\n        {\n          \"id\": 22,\n          \"nom\": \"Bo√Ætier de direction\",\n          \"prix\": 300,\n          \"categorie\": \"Direction\",\n          \"Image\": \"image_bo√Ætier_direction.jpg\",\n          \"Disponible\": true\n        },\n        {\n          \"id\": 23,\n          \"nom\": \"Silent bloc de suspension\",\n          \"prix\": 35,\n          \"categorie\": \"Suspension\",\n          \"Image\": \"image_silent_bloc_suspension.jpg\",\n          \"Disponible\": true\n        },\n        {\n          \"id\": 24,\n          \"nom\": \"injecteur carburant\",\n          \"prix\": 160,\n          \"categorie\": \"Transmission\",\n          \"Image\": \"image_injecteur_carburant.jpg\",\n          \"Disponible\": true\n        },\n        {\n          \"id\": 25,\n          \"nom\": \"Sileimage cardan transmission\",\n          \"prix\": 50,\n          \"categorie\": \"Moteur\",\n          \"Image\": \"image_sileimage_cardan_transmission.jpg\",\n          \"Disponible\": true\n        }\n      ]\n    };\n  },\n  computed: {\n    // Liste des cat√©gories uniques\n    categories() {\n      return [...new Set(this.produits.map(produit => produit.categorie))];\n    },\n    // Produits filtr√©s et tri√©s\n    filteredProduits() {\n      let produits = this.produits;\n\n      // Filtrage par recherche\n      if (this.searchQuery) {\n        produits = produits.filter(produit =>\n          produit.nom.toLowerCase().includes(this.searchQuery.toLowerCase())\n        );\n      }\n\n      // Filtrage par cat√©gorie\n      if (this.filterByCategory !== 'all') {\n        produits = produits.filter(produit =>\n          produit.categorie === this.filterByCategory\n        );\n      }\n\n      // Filtrage par disponibilit√©\n      if (this.filterByAvailability === 'available') {\n        produits = produits.filter(produit => produit.Disponible);\n      } else if (this.filterByAvailability === 'unavailable') {\n        produits = produits.filter(produit => !produit.Disponible);\n      }\n\n      // Tri par prix\n      if (this.sortOrder === 'asc') {\n        produits.sort((a, b) => a.prix - b.prix); // Prix croissant\n      } else if (this.sortOrder === 'desc') {\n        produits.sort((a, b) => b.prix - a.prix); // Prix d√©croissant\n      }\n\n      return produits;\n    },\n    // Calcul du prix total du panier\n    prixTotal() {\n      return this.panier.reduce((total, item) => total + item.produit.prix * item.quantite, 0);\n    }\n  },\n  methods: {\n    // Ajouter un produit au panier\n    ajouterAuPanier(produit) {\n      const index = this.panier.findIndex(item => item.produit.id === produit.id);\n      if (index !== -1) {\n        // Si le produit est d√©j√† dans le panier, augmenter la quantit√©\n        this.panier[index].quantite++;\n      } else {\n        // Sinon, ajouter le produit au panier avec une quantit√© de 1\n        this.panier.push({ produit, quantite: 1 });\n      }\n    },\n    // Supprimer un produit du panier ou d√©cr√©menter la quantit√©\n    supprimerDuPanier(index) {\n      if (this.panier[index].quantite > 1) {\n        // Si la quantit√© est sup√©rieure √† 1, d√©cr√©menter la quantit√©\n        this.panier[index].quantite--;\n      } else {\n        // Si la quantit√© est √©gale √† 1, supprimer le produit du panier\n        this.panier.splice(index, 1);\n      }\n    }\n  }\n};\n</script>\n\n<style>\n/* üåü Style global */\nbody {\n  max-width: 1200px;\n  margin: auto;\n  padding: 16px;\n  font-family: 'Poppins', sans-serif;\n  background-color: #f8f9fa;\n}\n\n/* üåü En-t√™te */\nheader {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  background-color: #6C63FF;\n  color: white;\n  padding: 20px;\n  border-radius: 10px;\n  box-shadow: 0px 4px 8px rgba(0, 0, 0, 0.1);\n}\n\nheader .logo-container {\n  display: flex;\n  align-items: center;\n}\n\nheader img {\n  height: 50px;\n  margin-right: 12px;\n}\n\nheader h1 {\n  font-size: 28px;\n  font-weight: 600;\n  text-transform: uppercase;\n  letter-spacing: 2px;\n}\n\n/* Organisation principale */\nmain {\n  display: flex;\n  gap: 24px;\n  margin-top: 20px;\n}\n\n/*  Filtres */\n.filtres {\n  width: 280px;\n  padding: 20px;\n  background: white;\n  border-radius: 10px;\n  box-shadow: 0px 4px 10px rgba(0, 0, 0, 0.1);\n}\n\n.filtres h3 {\n  font-size: 18px;\n  font-weight: 600;\n  margin-bottom: 10px;\n  text-transform: uppercase;\n  color: #6C63FF;\n}\n\n.input-container {\n  display: flex;\n  align-items: center;\n  background: #f0f0f0;\n  border-radius: 8px;\n  padding: 10px;\n}\n\n.filtres input {\n  width: 100%;\n  border: none;\n  background: transparent;\n  padding: 8px;\n  font-size: 14px;\n  outline: none;\n}\n\n.filter-section {\n  margin-top: 20px;\n}\n\n.filter-section h4 {\n  font-size: 16px;\n  font-weight: 600;\n  margin-bottom: 10px;\n  color: #6C63FF;\n}\n\n.filter-section select {\n  width: 100%;\n  padding: 8px;\n  border-radius: 8px;\n  border: 1px solid #ddd;\n  font-size: 14px;\n}\n\n/*  Section Produits */\n.fiches {\n  flex-grow: 1;\n  display: flex;\n  flex-direction: column;\n  gap: 20px;\n}\n\n/*  Style des Produits */\n.produit {\n  background: white;\n  border-radius: 10px;\n  padding: 20px;\n  box-shadow: 0px 4px 10px rgba(0, 0, 0, 0.1);\n  text-align: center;\n  transition: transform 0.2s ease-in-out;\n}\n\n.produit:hover {\n  transform: scale(1.03);\n}\n\n.produit img {\n  max-width: 100px;\n  height: auto;\n  margin-bottom: 10px;\n}\n\n.produit h3 {\n  font-size: 18px;\n  font-weight: 600;\n}\n\n.prix {\n  font-size: 16px;\n  font-weight: bold;\n  color: #6C63FF;\n}\n\n.disponible {\n  color: 